{
    "manifest_version": 3,
    "name": "Pro Xy",
    "version": "1.0",
    "description": "A simple Chrome extension to apply proxy.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["popup.js"]
        }
      ],
    "permissions": ["storage", "proxy"]
  }
  